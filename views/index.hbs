     <div class="row ml-2 justify-content-sm-start" id="product_list">
      
     </div>

<div class="card-deck">
{{#each products as |product| }}
    

    
    <div class="card bg-light shadow-lg p-3 mb-5 bg-white rounded card border-dark  mb-3 mx-2 p-1" id="box_shadow"  style="width: 18rem; height: 31rem;">
      <img src="https://www.adazing.com/wp-content/uploads/2019/02/open-book-clipart-06-1-300x300.png" class="card-img-top product_image" alt="...">
      <div class="card-body bg-transparent border-dark">
        <h5 class="card-title product_name text-center" >{{product.name}}</h5>
        <h5 class="card-title product_name text-center">Rs.{{product.price}}</h5>
        <h6 class="card-subtitle mb-2 text-muted prodct_manufacturer text-center">{{product.publisher}}</h6>
    </div>


    <div class="card-footer bg-transparent border-dark d-flex bd-highlight mb-3">
          <a href="#" class="btn btn-primary btn-lg mr-auto p-2 bd-highlight">Buy Now</a>
          <a href="#" class="btn btn-primary btn-lg p-2 bd-highlight">Rent Now</a>
    </div>
    </div>
    {{/each}}
